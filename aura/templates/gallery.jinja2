	<div class="arr-prev">
		<button type="submit">
			<img src="static/images/arr-prev.png"/>
		</button>
	</div>
	<div class="arr-next">
		<button type="submit">
			<img src="static/images/arr-next.png"/>
		</button>
	</div>
</div> -->


{% endfor %}



<div class='Pager'>
	{{ paginator.pager() |safe }}
</div>
		
{% endif %}
		
{% endblock %}
